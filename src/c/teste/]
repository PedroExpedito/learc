#include<stdlib.h>

#include<stdio.h>

#include<math.h>

#include<string.h>

int main()

{

int matriz[3][3];

int c=0,x=0,y=0;

int l=0;

int mat=0;

for(c=0;c<=1;c++)

{

for(l=0;l<=1;l++)

{

printf("Digite um o numero da matriz[%i][%i]",c,l);

scanf("%i",&matriz[c][l]);

if(matriz[c][l]%2==0)

{

y=y++;

}

else

{

mat=mat++;

}

}

}

printf("\nA quantidade de numeros impar:%d",mat);

}
